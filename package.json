{
    "name": "node-api-challenge",
    "version": "1.0.0",
    "description": "Node API Challenge",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "server": "nodemon index.js",
        "test": "cross-env NODE_ENV=testing jest --watch --verbose --runInBand --silent",
        "migrateup": "knex migrate:up",
        "migratedown": "knex migrate:down",
        "latest": "knex migrate:latest",
        "rollback": "knex migrate:rollback",
        "resetdb": "knex migrate:rollback && knex migrate:latest && knex seed:run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LambdaSchool/web-sprint-challenge-build-a-web-api"
    },
    "author": "Lambda School",
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "knex": "^0.95.6",
        "sqlite3": "^5.0.2",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "eslint": "^7.27.0",
        "jest": "^27.0.4",
        "knex-cleaner": "^1.3.1",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3"
    }
}
